{
  "name": "pwag",
  "version": "0.1.0",
  "description": "CLI tool to generate a set of favicons and a web manifest for pwa.",
  "keywords": [
    "favicon",
    "pwa",
    "web"
  ],
  "author": "wighawag",
  "license": "Unlicense",
  "homepage": "https://github.com/wighawag/pwag",
  "repository": "https://github.com/wighawag/pwag.git",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": "bin/index.js",
  "files": [
    "lib/",
    "bin/"
  ],
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "commander": "^10.0.0",
    "sharp": "^0.32.0",
    "svgo": "^3.0.2",
    "to-ico": "^1.1.5"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/sharp": "^0.31.1",
    "@types/to-ico": "^1.1.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:dev": "tsc --watch --project tsconfig.json",
    "prepublishOnly": "npm run build",
    "huskify": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts --fix --ignore-path .gitignore --cache",
    "prettify": "prettier --write --ignore-path .gitignore --plugin-search-dir=."
  },
  "pnpm": {
    "overrides": {
      "jpeg-js@<0.4.0": ">=0.4.0",
      "jpeg-js@<0.4.4": ">=0.4.4",
      "minimist@<0.2.1": ">=0.2.1",
      "minimist@<0.2.4": ">=0.2.4",
      "mkdirp@<1.0.0": ">=1.0.0",
      "uuid@<7.0.0": ">=7.0.0"
    },
    "allowedDeprecatedVersions": {
      "request": "*",
      "har-validator": "*"
    }
  }
}
